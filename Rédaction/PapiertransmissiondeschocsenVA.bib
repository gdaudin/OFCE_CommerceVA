@techreport{Benkovskis2015,
	Abstract = {We propose a comprehensive decomposition of changes in a country's global market shares that accounts for the value added content of trade. We perform the analysis by combining two datasets \textendash{} highly disaggregated trade data from UN Comtrade with internationally integrated Supply and Use Tables from the WIOD database. The inclusion of international fragmentation alters the underlying story behind changes in market shares between 1995 and 2011. The ongoing global outsourcing affects market shares directly by shifting production from G7 to BRIC countries. Moreover, accounting for real providers of the value added alters the balance between price and non-price drivers of global market shares. Changes in relative quality of countries' exports are mainly due to intermediate inputs, not due to domestic production. For instance, the seemingly improved or unchanged relative quality of BRIC export goods largely arose from outsourcing rather than from improvements in the quality of domestic production. In most cases, the dynamics of the value-added market shares is dominated by price and cost factors.},
	Author = {Benkovskis, Konstantins and W{\"o}rz, Julia},
	File = {/Users/guillaumedaudin/Documents/Recherche/Articles Scanne{\~A}?s/Zotero/storage/3KRBS8QA/Made in China (revised, clean).pdf},
