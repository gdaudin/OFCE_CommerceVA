@TechReport{Vicardcoutstravailservices2014,
  Title                    = {{Les co{\^u}ts du travail des services domestiques incorpor{\'e}s aux exportations p{\`e}sent-ils sur la comp{\'e}titivit{\'e}-co{\^u}t ?}},
