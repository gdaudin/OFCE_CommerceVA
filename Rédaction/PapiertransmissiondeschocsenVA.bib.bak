@Article{BlochlVertexCentralitiesInputOutput2011,
  Title                    = {Vertex {{Centralities}} in {{Input}}-{{Output Networks Reveal}} the {{Structure}} of {{Modern Economies}}},
  Author                   = {Bl{\"o}chl, Florian and J.Theis, Fabian and Vega-Redondo, Fernando and O'N.Fisher, Eric},
  Journal                  = {Physical Review},
  Year                     = {2011},
  Number                   = {83(4 Pt 2):046127},
  Pages                    = {9},
  Volume                   = {04/2011},

  Abstract                 = {Input-output tables describe the flows of goods and services between the sectors of an economy. These tables can be interpreted as weighted directed networks. At the usual level of aggregation, they contain nodes with strong self-loops and are almost completely connected. We derive two measures of node centrality that are well suited for such networks. Both are based on random walks and have interpretations as the propagation of supply shocks through the economy. Random walk centrality reveals the vertices most immediately affected by a shock. Counting betweenness identifies the nodes where a shock lingers longest. The two measures differ in how they treat self-loops. We apply both to data from a wide set of countries and uncover salient characteristics of the structures of these national economies. We further validate our indices by clustering according to sectors' centralities. This analysis reveals geographical proximity and similar developmental status. 

Vertex centralities in input-output networks reveal the structure of modern economies (PDF Download Available). Available from: http://www.researchgate.net/publication/51152966\_Vertex\_centralities\_in\_input-output\_networks\_reveal\_the\_structure\_of\_modern\_economies [accessed Jun 29, 2015].},
  Doi                      = {10.1103},
  File                     = {/Users/guillaumedaudin/Documents/Recherche/Articles ScanneÌ?s/Zotero/storage/7DXRER8J/BlÃ¶chl et al. - 2011 - Vertex Centralities in Input-Output Networks Revea.pdf},
  Keywords                 = {Analyse de rÃ©seau},
  Language                 = {English}
}

