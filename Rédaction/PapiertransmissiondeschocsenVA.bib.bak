@Article{DietzenbacherConstructionWorldInputOutput2013,
  Title                    = {The {{Construction}} of {{World Input}}-{{Output Tables}} in the {{WIOD Project}}},
  Author                   = {Dietzenbacher, Erik and Los, Bart and Stehrer, Robert and Timmer, Marcel and {de Vries}, Gaaitzen J.},
  Journal                  = {Routledge},
  Year                     = {2013},
  Number                   = {1},
  Pages                    = {71--98},
  Volume                   = {25},

  Abstract                 = {This article describes the construction of the World Input\textendash{}Output Tables (WIOTs) that constitute the core of the
World Input\textendash{}Output Database. WIOTs are available for the period 1995\textendash{}2009 and give the values of transactions
among 35 industries in 40 countries plus the `Rest of the World' and from these industries to households, gov-
ernments and users of capital goods in the same set of countries. The article describes how information from the
