  Author                   = {Akkemik, K. Ali},
  Journal                  = {Energy Policy},
  Year                     = {2011},

  Month                    = feb,
  Number                   = {2},
  Pages                    = {854--864},
  Volume                   = {39},

  Abstract                 = {Recent reforms in the Turkish electricity sector since 2001 aim to introduce a tariff system that reflects costs. This is expected to affect the production and consumer prices of electricity. The changes in electricity prices are then reflected in production costs in other segments of the economy. Subsequently, producer and consumer prices will be affected. The potential impact of the changes in electricity prices that the ongoing electricity reforms in Turkey will bring about may have important implications on the price formation in economic activities and the cost of living for households. This paper evaluates the potential impacts of changes in electricity prices from a social accounting matrix (SAM) price modeling perspective. It is found that based on the estimated price multipliers that prices in the energy-producing sectors, mining, and iron and steel manufacturing sectors would be affected more severely than the remaining sectors of the economy. Consumer prices are affected slightly less than producer prices.},
  Doi                      = {10.1016/j.enpol.2010.11.005},
  File                     = {/Users/guillaumedaudin/Documents/Recherche/Articles ScanneÌ?s/Zotero/storage/8JD5FHFM/Akkemik - 2011 - Potential impacts of electricity price changes on .pdf;/Users/guillaumedaudin/Documents/Recherche/Articles ScanneÌ?s/Zotero/storage/MV8KNABP/S0301421510008116.html},
  ISSN                     = {0301-4215},
  Keywords                 = {Inputâ€“output tables,Choc offre},
  Series                   = {Special Section on Offshore wind power planning, economics and environment},
  Shorttitle               = {Potential Impacts of Electricity Price Changes on Price Formation in the Economy},
  Urldate                  = {2015-06-25}
}

@TechReport{AmadorGlobalvaluechains2015,
  Title                    = {Global Value Chains: A View from the Euro Area},
  Author                   = {Amador, Joao and Cappariello, Rita and Stehrer, Robert},
  Institution              = {{ECB}},
  Year                     = {2015},

  Address                  = {Frankfurt},
  Month                    = mar,
  Number                   = {1761},
  Type                     = {Working Paper Series},

  Abstract                 = {This paper describes the main features of Global Value Chains (GVCs) in the euro area taken
as a whole and compares with other large trade players like the US, China and Japan. In addition,
the perspective of individual euro area countries is considered, with a focus on intra euro area
linkages. The analysis relies primarily on the concept of foreign value added in exports, as a
way to assess the pervasiveness of GVCs, it covers the period 2000-2011 and bases on the World
Input-Output Database (WIOD). The paper finds that GVCs are important for the euro area
as whole and they have rebounded after the great trade collapse. Moreover, there is a strong
relevance of regional production linkages in Europe, with Germany playing a key role.},
