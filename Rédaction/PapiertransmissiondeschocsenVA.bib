	Doi = {10.1103},
	File = {/Users/guillaumedaudin/Documents/Recherche/Articles Scanne{\~A}?s/Zotero/storage/7DXRER8J/Bl√∂chl et al. - 2011 - Vertex Centralities in Input-Output Networks Revea.pdf},
