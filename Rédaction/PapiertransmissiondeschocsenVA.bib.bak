  ISSN                     = {0022-1996},
  Keywords                 = {Production sharing,Inputâ€“output tables,Domestic content,Vertical specialization,Value added,Chocs de demande},
  Shorttitle               = {Accounting for Intermediates},
  Urldate                  = {2015-04-30}
}

